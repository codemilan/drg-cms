
<div class="dc-table-frame">

  <%= dc_table_title_for_result @records %>

  <% unless @records.nil? %>
    <table class="dc-result">
      <tr><%= dc_header_for_result %></tr>

      <% for r in @records %>
        <tr class="dc-<%= cycle("odd","even")%>" <%= dc_clicks_for_result r %> >
        <%= dc_actions_for_result r %>
        <%= dc_columns_for_result r %>
          
        </tr>
      <% end %>
        
    </table>
  <% end %>
  
</div>
